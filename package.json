{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npx vite",
    "dev:wrangler": "wrangler pages dev dist/public --compatibility-date=2025-01-15 --d1=DB",
    "build": "npx vite build",
    "build:functions": "npx esbuild functions/**/*.ts --bundle --outdir=dist/functions --format=esm --external:@cloudflare/* --platform=browser",
    "build:cf": "npm run build && npm run build:functions",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "npx tsc",
    "db:push": "npx drizzle-kit push",
    "db:migrate:d1": "npx drizzle-kit generate --config=drizzle.config.d1.ts && wrangler d1 migrations apply mate-music-db --local",
    "db:seed:d1": "bash scripts/seed-d1.sh",
    "cf:deploy": "npm run build:cf && wrangler pages deploy dist/public",
    "cf:dev": "wrangler pages dev dist/public --compatibility-flags nodejs_compat"
  },
